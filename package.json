{
  "name": "course-management-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "typeorm": "typeorm-ts-node-esm -d ./src/db/data-source.ts",
    "db:create": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs -d ./src/db/data-source.ts db:create",
    "db:drop": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs -d ./src/db/data-source.ts db:drop",
    "db:migration:generate": "npm run typeorm migration:generate",
    "db:migration:run": "npm run typeorm migration:run",
    "db:migration:up": "npm run db:create && npm run db:migration:run",
    "db:seeds:up": "ts-node ./src/db/seeds/index.ts",
    "start:prod": "node dist/index.js",
    "start:dev": "nodemon -L src/index.ts",
    "compose:up:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "compose:db:up:dev": "docker-compose -f docker-compose.dev.yml start db",
    "compose:backend:up:dev": "docker-compose -f docker-compose.dev.yml start backend",
    "compose:migration:up:dev": "docker-compose -f docker-compose.dev.yml start migrations",
    "compose:migration:generate": "docker-compose -f docker-compose.dev.yml run migrations npm run db:migration:generate",
    "compose:seeds:up": "docker-compose -f docker-compose.dev.yml run migrations npm run db:seeds:up",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-typeorm": "^2.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "http-errors": "^2.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "typeorm-extension": "^3.0.1"
  },
  "devDependencies": {
    "@jorgebodega/typeorm-seeding": "^6.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/node": "^20.5.2",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
